ick', function() {\r\n            if (currentActive < smallBoxes.length - 1) {\r\n                updateActiveBox(currentActive + 1);\r\n            }\r\n        });\r\n\r\n        function updateActiveBox(newIndex) {\r\n            smallBoxes.forEach(box => {\r\n                box.classList.remove('active');\r\n            });\r\n\r\n            smallBoxes[currentActive].classList.add('active');\r\n\r\n            setTimeout(() => {\r\n                smallBoxes[currentActive].classList.remove('active');\r\n\r\n                setTimeout(() => {\r\n                    smallBoxes[newIndex].classList.add('active');\r\n                    currentActive = newIndex;\r\n                }, 300);\r\n            }, 300);\r\n\r\n            const arrow = newIndex < currentActive ? upArrow : downArrow;\r\n            arrow.style.transform = 'scale(0.9)';\r\n            setTimeout(() => {\r\n                arrow.style.transform = 'scale(1)';\r\n            }, 200);\r\n        }\r\n    });\r\n\r\n    function selectProduct(element) {\r\n        const allProducts = document.querySelectorAll('.first-four-sold-catalog, .second-four-sold-catalog, .third-four-sold-catalog, .four-four-sold-catalog');\r\n        allProducts.forEach(product => {\r\n            product.classList.remove('product-selected', 'show-add-to-cart');\r\n        });\r\n\r\n        element.classList.add('product-selected');\r\n\r\n        setTimeout(() => {\r\n            element.classList.add('show-add-to-cart');\r\n        }, 200);\r\n\r\n        const addToCartBtn = element.querySelector('.add-to-cart');\r\n        addToCartBtn.addEventListener('click', function(e) {\r\n            e.stopPropagation();\r\n            alert('Товар добавлен в корзину!');\r\n        });\r\n    }\r\n\r\n    document.addEventListener('click', function(e) {\r\n        if (!e.target.closest('.first-four-sold-catalog, .second-four-sold-catalog, .third-four-sold-catalog, .four-four-sold-catalog')) {\r\n            const allProducts = document.querySelectorAll('.first-four-sold-catalog, .second-four-sold-catalog, .third-four-sold-catalog, .four-four-sold-catalog');\r\n            allProducts.forEach(product => {\r\n                product.classList.remove('product-selected', 'show-add-to-cart');\r\n            });\r\n        }\r\n    });\r\n    let currentlySelected = null;\r\n\r\n    function selectProduct(element) {\r\n        if (currentlySelected === element) {\r\n            element.classList.remove('product-selected', 'show-cart-btn');\r\n            currentlySelected = null;\r\n            return;\r\n        }\r\n\r\n        if (currentlySelected) {\r\n            currentlySelected.classList.remove('product-selected', 'show-cart-btn');\r\n        }\r\n\r\n        element.classList.add('product-selected');\r\n\r\n        setTimeout(() => {\r\n            element.classList.add('show-cart-btn');\r\n        }, 200);\r\n\r\n        currentlySelected = element;\r\n\r\n        const addToCartBtn = element.querySelector('.add-to-cart-btn');\r\n        addToCartBtn.onclick = function(e) {\r\n            e.stopPropagation();\r\n            alert('Товар \"' + element.querySelector('.Name-product').textContent + '\" добавлен в корзину!');\r\n        };\r\n    }\r\n\r\n    // Закрываем при клике вне товара\r\n    document.addEventListener('click', function(e) {\r\n        if (!e.target.closest('.first-four-sold-catalog, .second-four-sold-catalog, .third-four-sold-catalog, .four-four-sold-catalog')) {\r\n            if (currentlySelected) {\r\n                currentlySelected.classList.remove('product-selected', 'show-cart-btn');\r\n                currentlySelected = null;\r\n            }\r\n        }\r\n    });\r\n</script>\r\n</body>\r\n</html>\r\n
